{
	"name": "authenticator",
	"version": "0.0.1",
	"description": "Projeto que visa estudar conceitos de código implementando um sistema de autentificação",
	"main": "index.ts",
	"scripts": {
		"build": "tsc",
		"db:dev": "wsl docker compose up -d db-dev",
		"db:prod": "wsl docker compose up -d db-prod",
		"dev": "nodemon src/index.ts",
		"docker:dev": "wsl docker compose up dev",
		"docker:prod": "wsl docker compose up -d prod --build",
		"prod": "tsc & node ./dist/index.js",
		"start": "node ./dist/index.js",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
